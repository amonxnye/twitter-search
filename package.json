{
  "name": "twitter-search",
  "version": "0.1.0",
  "private": true,
  "description": "Algolia for searching your Tweets.",
  "repository": "saasify-sh/twitter-search",
  "author": "Saasify <dev@saasify.sh>",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "node build/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/server.ts",
    "predev": "run-s build:routes",
    "clean": "del build dist",
    "build": "run-s build:*",
    "prebuild": "run-s clean",
    "build:swagger": "tsoa swagger",
    "build:routes": "tsoa routes",
    "build:ts": "tsc",
    "test": "run-s build",
    "deploy": "now --prod",
    "predeploy": "run-s build",
    "postdeploy": "now rm --safe --yes twitter-search"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "algoliasearch": "^4.1.0",
    "dotenv": "^8.2.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^3.0.0",
    "koa-router": "^8.0.8",
    "lodash.pick": "^4.4.0",
    "tsoa": "^2.5.13",
    "twitter-lite": "^0.9.4"
  },
  "devDependencies": {
    "@types/algoliasearch": "^3.34.10",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-router": "^7.4.0",
    "@types/koa__cors": "^3.0.1",
    "@types/lodash.pick": "^4.4.6",
    "@types/node": "^13.11.1",
    "cross-env": "^7.0.2",
    "del-cli": "^3.0.0",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
