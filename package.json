    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^3.0.0",
    "koa-router": "^10.0.0",
    "lodash.pick": "^4.4.0",
    "p-retry": "^4.3.0",
    "tsoa": "^3.5.2",
    "twitter-lite": "^1.1.0"
  },
  "devDependencies": {
    "@types/algoliasearch": "^3.34.10",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-router": "^7.4.0",
    "@types/koa__cors": "^3.0.1",
    "@types/lodash.pick": "^4.4.6",
